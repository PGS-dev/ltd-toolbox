{
  "name": "@ltd-toolbox/figma-parser",
  "version": "0.1.0-alpha.7",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "design system",
    "figma",
    "parser",
    "markdown",
    "styles",
    "fetch"
  ],
  "scripts": {
    "build": "pkgroll build && typedoc",
    "test": "vitest",
    "lint": "tsc && eslint ./src --ext ts --report-unused-disable-directives --max-warnings 0",
    "format": "prettier ./src/ --write",
    "codecheck": "tsc --noEmit && yarn lint && yarn prettier ./src --check"
  },
  "devDependencies": {
    "@figma/rest-api-spec": "patch:@figma/rest-api-spec@npm%3A0.10.0#~/.yarn/patches/@figma-rest-api-spec-npm-0.10.0-287584738a.patch",
    "@types/node": "^20.8.0",
    "@types/picomatch": "^2.3.2",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "pkgroll": "^2.1.1",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.0.2",
    "vitest": "^0.34.6",
    "wsrun": "^5.2.4"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "hastscript": "^9.0.0",
    "type-fest": "^4.12.0",
    "unist-builder": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
