{
  "name": "@ltd-toolbox/figma-parser",
  "version": "0.1.0-alpha.6",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "design system",
    "figma",
    "parser",
    "markdown",
    "styles",
    "fetch"
  ],
  "scripts": {
    "build": "yarn pkgroll build && yarn typedoc",
    "test": "yarn vitest",
    "lint": "tsc && eslint ./src --ext ts --report-unused-disable-directives --max-warnings 0",
    "format": "yarn prettier ./src/ --write",
    "codecheck": "tsc --noEmit && yarn lint && yarn prettier ./src --check"
  },
  "devDependencies": {
    "@figma/rest-api-spec": "^0.10.0",
    "@types/node": "^20.8.0",
    "@types/picomatch": "^2.3.2",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "pkgroll": "^2.1.1",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.0.2",
    "vitest": "^0.34.6",
    "wsrun": "^5.2.4"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "hastscript": "^9.0.0",
    "type-fest": "^4.12.0",
    "unist-builder": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./dev": {
      "import": {
        "types": "./dist/dev.d.ts",
        "default": "./dist/dev.js"
      },
      "require": {
        "types": "./dist/dev.d.cts",
        "default": "./dist/dev.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      },
      "require": {
        "types": "./dist/core/index.d.cts",
        "default": "./dist/core/index.cjs"
      }
    },
    "./document": {
      "import": {
        "types": "./dist/document/index.d.ts",
        "default": "./dist/document/index.js"
      },
      "require": {
        "types": "./dist/document/index.d.cts",
        "default": "./dist/document/index.cjs"
      }
    },
    "./styles": {
      "import": {
        "types": "./dist/styles/index.d.ts",
        "default": "./dist/styles/index.js"
      },
      "require": {
        "types": "./dist/styles/index.d.cts",
        "default": "./dist/styles/index.cjs"
      }
    },
    "./variables": {
      "import": {
        "types": "./dist/variables/index.d.ts",
        "default": "./dist/variables/index.js"
      },
      "require": {
        "types": "./dist/variables/index.d.cts",
        "default": "./dist/variables/index.cjs"
      }
    },
    "./contents": {
      "import": {
        "types": "./dist/contents/index.d.ts",
        "default": "./dist/contents/index.js"
      },
      "require": {
        "types": "./dist/contents/index.d.cts",
        "default": "./dist/contents/index.cjs"
      }
    }
  }
}
