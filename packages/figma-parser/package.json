{
  "name": "@ltd-toolbox/figma-parser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn vite build --watch",
    "build": "yarn vite build"
  },
  "files": [
    "./dist/*"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.mjs",
      "require": "./dist/parser.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*/index.d.ts",
      "import": "./dist/plugins/*/index.mjs",
      "require": "./dist/plugins/*/index.cjs"
    },
    "./plugins/styles/transformsers/*": {
      "types": "./dist/plugins/styles/transformsers/*/index.d.ts",
      "import": "./dist/plugins/styles/transformsers/*/index.mjs",
      "require": "./dist/plugins/styles/transformsers/*/index.cjs"
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {
    "@types/mdast": "^4.0.1",
    "@types/node": "^20.7.1",
    "@types/picomatch": "^2.3.2",
    "flat": "^6.0.1",
    "mdast-builder": "^1.1.1",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "picomatch": "^2.3.1",
    "remark": "^15.0.1",
    "unist-builder": "^4.0.0"
  }
}
